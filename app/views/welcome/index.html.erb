<section>
  <% if current_user %>
    <%= button_to "Logout", logout_path, method: :get %>
  <% else %>
    <%= button_to "Create New User", register_path, data: {turbo: false}, method: :get %>
    <%= link_to "Log In", login_path %>
  <% end %>
</section>

<section class='existing-users'>
  <p>Existing Users: </p>
  <% @users.each do |user| %>
    <li><%= link_to user.email, user_path(user.id) %></li>
  <% end %>
</section>